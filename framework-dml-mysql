-- 권한 그룹 권한테이블에 추가
INSERT INTO USTRA_AUTH (AUTH_ID, AUTH_GRP_ID, AUTH_TY_CD, USE_SRT_DT, USE_END_DT, REG_DTTM, REG_USR_ID, REG_USR_IP, UPD_DTTM, UPD_USR_ID, UPD_USR_IP )
SELECT (SELECT COALESCE(MAX(AUTH_ID) + 1, 1) from USTRA_AUTH), AUTH_GRP_ID, '01', '20210101', '20221231', NOW(), 'INIT_DATA', '127.0.0.1', NOW(), 'INIT_DATA', '127.0.0.1'
FROM USTRA_AUTH_GRP
WHERE AUTH_GRP_NM ='시스템 관리자' AND USE_YN='Y' AND DEL_YN ='N';

-- 메뉴 권한 추가
INSERT INTO USTRA_MNU_AUTH
	(MNU_ID,
	AUTH_ID,
	REG_DTTM,
	REG_USR_ID,
	REG_USR_IP,
	UPD_DTTM,
	UPD_USR_ID,
	UPD_USR_IP
	)
SELECT B.MNU_ID,
	   A.AUTH_ID,
	   NOW(),
	  'INIT_DATA',
	  '127.0.0.1',
	  NOW(),
	  'INIT_DATA',
	  '127.0.0.1'
  FROM USTRA_AUTH A
  			CROSS JOIN USTRA_MNU B
 WHERE B.USE_YN = 'Y'
   AND B.DP_YN = 'Y'
   AND B.MNU_URL IS NOT NULL
   AND B.DEL_YN = 'N'
   AND A.AUTH_ID IN (
   	SELECT AUTH_ID
 	  FROM USTRA_AUTH_GRP_USR A
 	  		INNER JOIN USTRA_AUTH_GRP B
 	  			ON A.AUTH_GRP_ID = B.AUTH_GRP_ID
 	  		INNER JOIN USTRA_AUTH C
 	  			ON B.AUTH_GRP_ID = C.AUTH_GRP_ID
 	 WHERE B.USE_YN = 'Y'
   	   AND B.DEL_YN = 'N'
   	   AND A.USR_ID IN ('admin')
);

