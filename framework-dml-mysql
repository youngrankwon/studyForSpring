-- UPR MNU ID 업데이트
UPDATE USTRA_MNU SET UPR_MNU_ID = (SELECT temp.MNU_ID FROM (SELECT MNU_ID FROM USTRA_MNU WHERE SYS_CD = 'BO' AND MNU_NM = '시스템' AND MNU_STEP_NO = 1) AS temp) WHERE UPR_MNU_ID IS NULL AND MNU_STEP_NO = 2;

